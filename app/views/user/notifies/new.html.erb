<!-- start: Content -->
<div id="content" class="span10" style="min-height: 600px;">
  <div class="row-fluid">
    <div class="box span12">
      <div class="box-header">
        <h2><i class="icon-edit"></i>Add notify for book number</h2>
        <div class="box-icon">
        </div>
      </div>
      <div class="box-content">
        <%= simple_form_for(@notify, :url => user_notifies_path, 
        :html => { :method => :post, :class=>"form-horizontal" } ) do |f| %>
          <%= render :partial => "form", :locals => { :f => f } %>
        <% end %>
      </div>
    </div><!--/span-->
  </div><!--/row-->
</div>
<!-- end: Content -->

<!-- start: Content -->
<div id="content" class="span10">
  <div class="row-fluid">
    <div class="box span12">
      <div class="box-header">
        <h2><i class="icon-font"></i><span class="break"></span><%= @user.full_name%></h2>
      </div>
      <div class="box-content">
        <div class="page-header">
          <h1><small>Please have a look into profile</small></h1>
        </div>     
        <div class="row-fluid">            
          <%= render :partial => 'profile' %>
          <div class="form-actions">
            <%= link_to 'Update Profile', edit_user_registration_path, :class => 'btn btn-primary' %>  
            <%= link_to 'Back', user_dashboard_index_path, :class => 'btn btn-inverse' %>
          </div>
        </div>
      </div>
    </div>
  </div><!--/row-->      
</div>
<!-- end: Content -->